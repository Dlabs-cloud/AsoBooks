{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2019",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@dlabs/payment": [
        "libs/payment/src"
      ],
      "@dlabs/payment/*": [
        "libs/payment/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}