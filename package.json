{
  "name": "asobooks.com",
  "version": "0.0.1",
  "engines": {
    "node": "12.18.1",
    "npm": "6.14.5"
  },
  "description": "",
  "author": "support@dlabs.cloud",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prestart:prod": "rimraf dist && npm run build",
    "postinstall": "npm run prestart:prod",
    "worker:start": "ts-node -r tsconfig-paths/register src/worker.ts",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
    "format": "prettier --write \\\\\\\"src/**/*.ts\\\\\\\" \\\\\\\"views/test/**/*.ts\\\\\\\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --max-old-space-size=8192 dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "ENV=test jest --runInBand",
    "test:watch": "ENV=test jest --watch",
    "test:cov": "ENV=test jest --coverage --runInBand",
    "test:debug": "ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "ENV=test jest --runInBand  --config  src/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/bull": "^0.2.2",
    "@nestjs/common": "^6.11.11",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^6.11.11",
    "@nestjs/cqrs": "^7.0.0",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/typeorm": "^7.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^4.1.12",
    "@types/glob": "7.1.3",
    "@types/inline-css": "0.0.33",
    "@types/jest": "26.0.4",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "4.14.157",
    "@types/multer": "^1.4.3",
    "@types/nodemailer": "6.4.0",
    "@types/pug": "2.0.4",
    "@types/stream-json": "^1.5.1",
    "aws-sdk": "^2.749.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.18.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "faker": "^4.1.0",
    "handlebars": "^4.7.6",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "nest-winston": "^1.3.5",
    "nestjs-early-access": "^1.0.4",
    "nestjs-s3": "^1.0.1",
    "nodemailer": "^6.4.10",
    "npm": "^6.14.6",
    "optional-typescript": "github:Dlabs-cloud/Optional#feature/optional-if-present",
    "pg": "^8.3.3",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "stream-json": "^1.7.0",
    "typeorm": "^0.2.25",
    "winston": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.11.11",
    "@types/bull": "^3.14.2",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.4",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
